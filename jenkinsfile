pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                checkout(
                    [
                        $class: 'GitSCM',
                        branches: [[name: 'main']],
                        userRemoteConfigs: [[credentialsId: 'jenkins-token', url: 'https://github.com/Nimraaa18/jenkins-repo.git']]
                    ]
                )
            }
        }

        stage('Hello') {
            steps {
                echo 'Hello Jenkins'
            }
        }
    }
}

